<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="My_Suite">
	
	
	
  <test thread-count="3" name="chrome">
	  
	  <parameter name="browserName" value="chrome"/>
	  
    <classes>
      <class name="testRunner.TestNgRunner"/>
    </classes>
    
  </test> <!-- LogIn_Test -->
  
  
